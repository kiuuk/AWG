<div id="wods-index">
  <wods-index>
  </wods-index>
</div>
<%= javascript_pack_tag 'common'%>
<%= javascript_pack_tag 'wods/index'%>

<%
=begin%>
 <p id="notice"><%= notice %></p>

<h1>Wods</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Score</th>
      <th>Score set</th>
      <th>Rep</th>
      <th>Description</th>
      <th>Movement 1</th>
      <th>Movement 2</th>
      <th>Movement 3</th>
      <th>Movement 4</th>
      <th>Movement 5</th>
      <th>Movement 6</th>
      <th>Movemtnt 7</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wods.each do |wod| %>
      <tr>
        <td><%= wod.name %></td>
        <td><%= wod.score %></td>
        <td><%= wod.score_set %></td>
        <td><%= wod.rep %></td>
        <td><%= wod.description %></td>
        <td><%= wod.movement_1 %></td>
        <td><%= wod.movement_2 %></td>
        <td><%= wod.movement_3 %></td>
        <td><%= wod.movement_4 %></td>
        <td><%= wod.movement_5 %></td>
        <td><%= wod.movement_6 %></td>
        <td><%= wod.movemtnt_7 %></td>
        <td><%= link_to 'Show', wod %></td>
        <td><%= link_to 'Edit', edit_wod_path(wod) %></td>
        <td><%= link_to 'Destroy', wod, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wod', new_wod_path %> 
<%
=end%>
